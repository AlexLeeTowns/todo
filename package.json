  "eslintConfig": {
    "env": {
      "jest/globals": true
    },
    "extends": [
      "airbnb-base"
    ],
    "plugins": [
      "jest",
      "import/resolver"
    ],
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "linebreak-style": [
        "error",
        "windows"
      ]
    },
    "settings": {
      "import/resolver": {
        "babel-module": {}
      }
    }
  },
  "jest": {
    "testEnvironment": "node",
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Test Report"
        }
      ]
    ]
  }
